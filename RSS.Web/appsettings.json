{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  //CO2NET 设置
  "SenparcSetting": {
    //以下为 CO2NET 的 SenparcSetting 全局配置，请勿修改 key，勿删除任何项

    "IsDebug": true,
    "DefaultCacheNamespace": "DefaultCache",
    //分布式缓存
    "Cache_Redis_Configuration": "Redis配置",
    //"Cache_Redis_Configuration": "localhost:6379",
    "Cache_Memcached_Configuration": "Memcached配置",
    "SenparcUnionAgentKey": "SenparcUnionAgentKey"
  },
  //Senparc.Weixin SDK 设置
  "SenparcWeixinSetting": {
    //以下为 Senparc.Weixin 的 SenparcWeixinSetting 微信配置

    //微信全局
    "IsDebug": true,

    //小程序
    "WxOpenAppId": "appid",
    "WxOpenAppSecret": "secret",
    "WxOpenToken": "",
    "WxOpenEncodingAESKey": ""
  },
  "ConnectionStrings": {
     "DefaultConnection": "server=127.0.0.1;Database=rssapp;Uid=rssapp;Pwd=12345678;Pooling=true;Max Pool Size =100;Min Pool Size = 10;Charset=utf8mb4;SslMode=none"
 }
}
